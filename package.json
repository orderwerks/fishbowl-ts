{
  "name": "fishbowl-ts",
  "version": "1.0.0",
  "description": "A TypeScript wrapper for the Fishbowl API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Orderwerks/fishbowl-ts.git"
  },
  "keywords": [
    "fishbowl",
    "inventory",
    "fishbowljs",
    "js-fishbowl",
    "node-fishbowl",
    "fishbowlnode",
    "plugin",
    "fishbowlts",
    "ts-fishbwol"
  ],
  "author": "Stephen Bero",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Orderwerks/fishbowl-ts/issues"
  },
  "homepage": "https://github.com/Orderwerks/fishbowl-ts#readme",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/node": "^15.12.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "prettier": "^2.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "jquery-csv": "^1.0.21",
    "winston": "^3.3.3"
  }
}
